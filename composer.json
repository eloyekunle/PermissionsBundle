{
    "name": "eloyekunle/permissions-bundle",
    "description": "A Simple Permission Management Bundle for Symfony",
    "type": "symfony-bundle",
    "keywords": ["User management"],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "symfony/framework-bundle": "^2.7 || ^3.0 || ^4.0",
        "symfony/form": "^2.7 || ^3.0 || ^4.0",
        "symfony/yaml": "^2.7 || ^3.0 || ^4.0",
        "symfony/validator": "^2.8 || ^3.0 || ^4.0",
        "symfony/security-bundle": "^2.8 || ^3.0 || ^4.0",
        "symfony/twig-bundle": "^2.8 || ^3.0 || ^4.0",
        "symfony/templating": "^2.8 || ^3.0 || ^4.0",
        "twig/twig": "^1.28 || ^2.0",
        "paragonie/random_compat": "^1 || ^2"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.9",
        "friendsofphp/php-cs-fixer": "^2.2",
        "symfony/phpunit-bridge": "^2.8 || ^3.0 || ^4.0",
        "doctrine/orm": "^2.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Elijah Oyekunle",
            "email": "eloyekunle@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Eloyekunle\\PermissionsBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Eloyekunle\\PermissionsBundle\\Tests\\": "/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
